{
  "name": "Mr.Taskman",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon",
    "start": "node ./build/index.js",
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc",
    "lint": "eslint ./src --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix",
    "verify": "npm run lint:fix && npm test",
    "test:": "jest",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run verify"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "discord.js": "^12.5.0",
    "dotenv": "^8.2.0",
    "mongoose": "^5.10.17"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
